// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			
			
			"type": "npm",
			"script": "watch",
			"isBackground": true,
			"problemMatcher": {
				"owner": "typescript",
				"fileLocation": "relative",
				"pattern": {
				  "regexp": "^(.*)$",
				  "file": 1,
				},
				"background": {
				  "activeOnStart": true,
				  "beginsPattern": "^(> npm run esbuild-base).*",
				  "endsPattern": "^\\[watch\\].*"
				}
			},
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}